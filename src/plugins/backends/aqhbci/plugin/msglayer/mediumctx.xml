      <header type="sys" >gwenhywfar/crypttoken.h</header>
      <header type="sys" >aqhbci/aqhbci.h</header>
      <header type="sys" >aqhbci/medium.h</header>
    </headers>
    
    <elem name="user" type="GWEN_CRYPTTOKEN_USER" ptr="1" const="0" />
    <elem name="tokenContext" type="GWEN_CRYPTTOKEN_CONTEXT" ptr="1" const="0" />
    <elem name="localSignKeySpec" type="GWEN_KEYSPEC" ptr="1"/>
    <elem name="localCryptKeySpec" type="GWEN_KEYSPEC" ptr="1"/>
    <elem name="remoteSignKeySpec" type="GWEN_KEYSPEC" ptr="1"/>
    <elem name="remoteCryptKeySpec" type="GWEN_KEYSPEC" ptr="1"/>
  </type>
  
</types>
